@using BandsMvc.Models;
@model Band[]
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    <h3>Alla Band</h3>
    <div>
        <ul>
            @foreach (var item in Model)
            {
                <li> Id: @item.Id. Band: @item.Name. Description: @item.Description <a href="details/@item.Id">Länk till bandet</a></li>
            }

        </ul>
    </div>
</body>
</html>